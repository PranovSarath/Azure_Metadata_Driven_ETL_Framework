{
	"name": "DS_CSV_RawOutput",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_Az_ADLS_Gen2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"DataHistoryID": {
				"type": "int"
			},
			"DestLayer": {
				"type": "string"
			},
			"EntityName": {
				"type": "string"
			},
			"RootFolderName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().EntityName, '.csv')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().DestLayer,'/',dataset().RootFolderName,'/', string(dataset().DataHistoryID), '/', dataset().EntityName)",
					"type": "Expression"
				},
				"fileSystem": "raw"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}